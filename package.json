		"@angular/platform-browser-dynamic": "^18.2.8",
		"@angular/router": "^18.2.8",
		"@types/node": "^20.1.5",
		"ngx-image-cropper": "^7.1.1",
		"rxjs": "~7.5.0",
		"socket.io-client": "^4.5.1",
